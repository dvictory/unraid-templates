?xml version="1.0"?>
<Container version="2">
  <Name>ChiaNode</Name>
  <Repository>ghcr.io/chia-network/chia:latest</Repository>
  <Registry/>
  <Network>semimetallic</Network>
  <MyIP/>
  <Shell>bash</Shell>
  <Privileged>false</Privileged>
  <Support/>
  <Project/>
  <Overview/>
  <Category>Security: Tools: Other:</Category>
  <WebUI/>
  <TemplateURL/>
  <Icon>https://pbs.twimg.com/profile_images/1106297914924523520/zV5K5y9f_400x400.png</Icon>
  <ExtraParams>--hostname=chia</ExtraParams>
  <PostArgs/>
  <CPUset/>
  <DateInstalled>1619529523</DateInstalled>
  <DonateText/>
  <DonateLink/>
  <Description/>
  <Networking>
    <Mode>semimetallic</Mode>
    <Publish/>
  </Networking>
  <Data>
    <Volume>
      <HostDir>/mnt/user/chia</HostDir>
      <ContainerDir>/plots</ContainerDir>
